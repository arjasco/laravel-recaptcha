{
    "name": "arjasco/laravel-recaptcha",
    "description": "Easily add reCAPTCHA to your Laravel projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Costello",
            "email": "aaronjamescos@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Arjasco\\LaravelRecaptcha\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arjasco\\LaravelRecaptcha\\RecaptchaServiceProvider"
            ]
        }
    },
    "require": {
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0"
    }
}
